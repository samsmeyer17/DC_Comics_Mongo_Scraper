<div class="Title" id="weekly-comics-header">
  <h3>
    This Weeks Comics
  </h3>
</div>

{{#each articles}}
{{#unless saved}}

<div class="col s12 m6">
  <div class="card">
    <div class="card-image">
      <img src="{{{img}}}"></img>
      <a class="btn-floating halfway-fab waves-effect waves-light red btn modal-trigger" href="note-modal">&#129300;</a>
    </div>
    <div class="card-content">
      <p>{{{title}}}</p>
      <button data-id={{_id}} class="save-article-btn">Save Comic</button>
    </div>
  </div>
</div>

{{/unless}}
{{/each}}